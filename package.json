{
  "name": "protractor-cucumber-typescript-boilerplate",
  "version": "3.0.1",
  "description": "A boilerplate for starting a Protractor test setup with CucumberJS and TypeScript",
  "main": "index.js",
  "scripts": {
    "updateDrivers": "./node_modules/.bin/webdriver-manager.cmd update",
    "downFirefox": "./node_modules/.bin/webdriver-manager.cmd update --versions.gecko=v0.18.0",
    "e2e": "protractor e2e-tests/config/protractor.e2e.conf.js",
    "flake": "node e2e-tests/config/protractor.flake.js",
    "flakeMultiCap": "node e2e-tests/config/protractor.flake.js",
    "travis": "protractor e2e-tests/config/protractor.travis.conf.js",
    "postinstall": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wswebcreation/protractor-cucumber-typescript-boilerplate.git"
  },
  "keywords": [
    "protractor",
    "cucumber",
    "cucumberjs",
    "typescript",
    "boilerplate"
  ],
  "author": "wswebcreation",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wswebcreation/protractor-cucumber-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/wswebcreation/protractor-cucumber-typescript-boilerplate#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cucumber": "^2.0.3",
    "@types/fs-extra": "^4.0.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "~3.0.1",
    "cucumber-html-reporter": "~0.3.2",
    "fs-extra": "^4.0.1",
    "geckodriver": "^1.12.2",
    "protractor": "5.1.2",
    "protractor-cucumber-framework": "~4.1.1",
    "protractor-flake": "^3.0.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.1.0",
    "selenium-webdriver": "^3.6.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "yargs": "^8.0.2"
  }
}
